<div>
    {{#with main}}
    <main>
        <section>
            {{>title data='Education' big=true}}
            <ul>
                {{#each education}}
                <li>{{name}}


                  
                     {{#if isEnd}}  {{/if}}
                        {{#unless isEnd includeZero=false}} (ще в процесі) {{/unless}}
                        
                </li>
                {{/each}}
            </ul>
        </section>

        <section>
            {{>title data= "Sertificates"}}
            <ul>
                {{#each sertificates}}
                <li>
                    {{name}}

                     {{#if id}} ({{id}}) {{/if}}
                        
                </li>
                {{/each}}
            </ul>
        </section>
    </main>
    {{/with}}

</div>